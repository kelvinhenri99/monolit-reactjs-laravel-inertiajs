{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
		"prettier/prettier": "error",
		"react/prop-types": ["off"],
    "quotes": ["error", "single"],
    "semi": ["error", "never"]
  }
}
